LEXICON NOUN_SGPAR_TU

+Sg+Par:tu # ;

LEXICON NOUN_SGILL_H

+Sg+Ill:h # ;

LEXICON NOUN_PLGEN_IEN

+Pl+Gen:ien # ;

LEXICON NOUN_PLGEN_IJEN

